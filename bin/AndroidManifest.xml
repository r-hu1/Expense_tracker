<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ca.ualberta.cs.expensetracker"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ListClaimActivity"
            android:label="@string/title_activity_list_claim" >
        </activity>
        <activity
            android:name=".ViewClaimActivity"
            android:label="@string/title_activity_view_claim" >
        </activity>
        <activity
            android:name=".CreatNewClaimActivity"
            android:label="@string/title_activity_creat_new_claim" >
        </activity>
        <activity
            android:name=".ClaimItemActibvity"
            android:label="@string/title_activity_claim_item_actibvity" >
        </activity>
        <activity
            android:name=".LauncherMainActivity"
            android:label="@string/title_activity_launcher_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
